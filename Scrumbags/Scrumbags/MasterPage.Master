<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="MasterPage.Master.cs" Inherits="Scrumbags.MasterPageClass" %>

<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
    <title>Scrumbags</title>
    <link href="StyleSheet.css" rel="stylesheet" type="text/css" />
    <link href="favicon.ico" rel="icon" type="image/x-icon" />
    <asp:ContentPlaceHolder ID="head" runat="server">
    </asp:ContentPlaceHolder>
</head>
<body>
    <form id="masterform" runat="server">
    <div id="masterHeader">
        <img src="AP.png" id="headerLogo" />
        <asp:Button ID="logoutButton" CssClass="headerButton" runat="server" OnClick="LogoutButton_Click" Text="Log out" /> 
        <asp:Button ID="ReservationsHyperLink" CssClass="headerLink" runat="server" OnClick="ReservationsHyperLink_Click" Text="Check my reservations" />
    </div>
    <div>
        <asp:Label ID="siteMessage" runat="server"></asp:Label>
        <asp:ContentPlaceHolder ID="MasterContentPlaceHolder" runat="server">
            <!-- Niks inzetten, placeholder voor content pages -->
        </asp:ContentPlaceHolder>
    </div>
    </form>
</body>
</html>
